    "import cv2\n",
    "import numpy as np\n",
    "img=cv2.imread(r\"R:\\Mobile's Photo\\DCIM\\Camera\\IMG_20240404_111921.jpg\")\n",
    "img1=cv2.resize(img,(200,300))\n",
    "h=np.hstack((img1,img1))\n",
    "v=np.vstack((h,h))\n",
    "cv2.imshow(\"apu\",v)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\n",
    "cv2.imwrite(\"R:\\\\r.jpg\",v)"
